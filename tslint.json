{
  "extends": "tslint:recommended",
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "no-console": false,
    "jsx-no-lambda": false,
    "ordered-imports": false,
    "semicolon": [true, "always", "ignore-bound-class-methods"]
  },
  "jsRules": {
    "no-console": false

  }
}